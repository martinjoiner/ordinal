    "keywords": ["ordinal", "number", "english"],
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Joiner",
            "email": "martin@martinjoiner.co.uk",
            "homepage": "http://martinjoiner.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
    	"php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*"
    },
    "support": {
        "source": "https://github.com/martinjoiner/ordinal-number",
        "issues": "https://github.com/martinjoiner/ordinal-number/issues"
    },
    "autoload": {
        "psr-4": {
            "MartinJoiner\\OrdinalNumber\\": "src"
        }
    }
